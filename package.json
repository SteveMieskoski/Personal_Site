  "scripts": {
    "rimraf": "rimraf",
    "webpack": "webpack",
    "clean:dist": "npm run rimraf -- dist",
    "prebuild:prod": "npm run clean:dist",
    "build:webpack": "rimraf  ./dist/* ; webpack --config ./webpack.config.js  --progress --colors",
    "dev": "./node_modules/.bin/nodemon src/index.js --exec ./node_modules/.bin/babel-node --presets es2015",
    "test": "mocha ./tests/* --compilers js:babel-register",
    "lint": "eslint src --ignore-path ./src/boot/import-data.js",
    "run:local": "node server.js",
    "Mocha:Selenium": "sh ./bash_scripts/MochaSelenium.sh",
    "selenium": "webdriver-manager start",
    "webDriver": "sh bash_scripts/WebDriver.sh",
    "tsop:webDriver": " timestamp=$(date \"+%m%d%H%M\"); cp ./test/WebDriverIO_created/selenium_logs/selenium-standalone.txt ./test/WebDriverIO_created/selenium_logs/selenium-standalone_$(+%m%d%H%M).txt ; ",
    "webDriver:CleanImgs": "npm run rimraf -- test/errorShots; sh bash_scripts/WebDriver.sh",
    "cleanJsonReports": "npm run rimraf -- test/JSON_Reports",
    "AllureReport": "sh bash_scripts/allure.sh",
    "webDriver:Allure": "npm run webDriver; npm run AllureReport",
    "unitDemo": "node_modules/.bin/intern-client config=test/intern; exit 0;",
    "karma:start": "./node_modules/karma/bin/karma start",
    "karma:run": "./node_modules/karma/bin/karma run",
    "mocha": "mocha"
  },
